upstream django {
  server unix:///var/www/html/python/photoyatra/pyatra.sock;
}

server {
  listen 80 default_server;
  listen [::]:80 default_server ipv6only=on;

  root /usr/share/nginx/html;
  index index.html index.htm;

  server_name dev.photoyatra.in;

  location / {
    uwsgi_pass django;
    include /var/www/html/python/photoyatra/pyatra/conf/uwsgi_params;
  }

  location /static/ {
    alias /var/www/html/python/photoyatra/pyatra/staticfiles/;
  }

  location /media/ {
    alias /var/www/html/python/photoyatra/pyatra/media/;
  }
}